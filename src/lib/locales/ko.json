{
  "notifications": {
    "new_print": {
      "title": "새로운 프린트 도착!",
      "body_template": "\"{teaser}\" from {senderName}"
    },
    "friend_request": {
      "title": "새로운 친구 요청",
      "body_template": "{requesterName}님이 친구가 되고 싶어해요."
    },
    "friend_accept": {
      "title": "친구 수락",
      "body_template": "{senderName}님과 친구가 되었어요."
    },
    "printer_connected": {
      "title": "프린터 연결됨",
      "body_template": "{printerName}와 연결되었습니다."
    },
    "printer_disconnected": {
      "title": "프린터 연결 끊김",
      "body": "프린터 연결이 끊어졌습니다. 다시 연결해주세요."
    },
    "print_completed": {
      "title": "프린트 완료",
      "body": "메시지가 성공적으로 프린트되었습니다."
    },
    "print_error": {
      "title": "프린트 오류",
      "body_template": "프린트 중 오류가 발생했습니다: {errorMessage}"
    },
    "default": {
      "title": "새 알림",
      "body": "새로운 알림이 도착했습니다."
    }
  },
  "fallbacks": {
    "unknown_user": "알 수 없는 사용자",
    "new_message": "새 메시지",
    "printer": "프린터",
    "unknown_error": "알 수 없는 오류"
  }
} 